#include<iostream>
using namespace std;
class complex{
private:
float real;
float img;
public:
complex(float r=0,float i=0):real(r),img(i){}
void display(){
cout<<real<<" + "<<img<<"i"<<endl;
}
complex operator-(){
return complex(-real,-img);
}
complex operator++(){
++real;
++img;
